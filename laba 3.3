#include <iostream>
#include <cmath>
using namespace std;

void m(double& a1, double& b1) {
	double temp;
	temp = a1;
	a1 = b1;
	b1 = temp;
}

int main() {
	setlocale(0, "");

	double a, b, c;

	cout << "Введите переменные A, B, C : " << endl;
	cin >> a >> b >> c;

	m(a, c);
	m(a, b);

	cout << "Результат :" << a << " " << b << " " << c;

	cout << endl;

	system("pause");

	return 0;

}
