#include <iostream>
#include <cmath>
using namespace std;

void m(double &a1, double &b1) {
	double temp;
	temp = a1;
	a1 = b1;
	b1 = temp;
}

int main() {
	setlocale(0, "");

	double a, b;

	cout << "Введите переменные A и В : " << endl;
	cin >> a >> b;

	m(a, b);

	cout << "Результат :" << a << " " << b;

	cout << endl;

	system("pause");
	
	return 0;

}
